<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Release Settings"
	OutputDirectory="$(BinDir)$(ConfigDir)$(PlatformDir)\"
	IntermediateDirectory="$(LibDir)$(ConfigDir)$(PlatformDir)\$(ProjectName)\"
	InheritedPropertySheets=".\DaCommon.vsprops"
	>
	<Tool
		Name="VCCLCompilerTool"
		Optimization="1"
		InlineFunctionExpansion="1"
		EnableIntrinsicFunctions="true"
		PreprocessorDefinitions="WIN32;_WINDOWS"
		StringPooling="true"
		RuntimeLibrary="0"
		EnableFunctionLevelLinking="true"
		DebugInformationFormat="3"
	/>
	<Tool
		Name="VCLinkerTool"
		LinkIncremental="1"
		GenerateDebugInformation="true"
		ProgramDatabaseFile="$(IntDir)$(TargetName).Pdb"
		StripPrivateSymbols="$(IntDir)$(TargetName).Stripped.Pdb"
		GenerateMapFile="true"
		MapFileName="$(IntDir)$(TargetName).Map"
		MapExports="true"
		OptimizeReferences="2"
		EnableCOMDATFolding="2"
	/>
	<Tool
		Name="VCPostBuildEventTool"
		Description="Signing the code..."
		CommandLine="signtool sign /n &quot;Cinnamon Software Inc.&quot; /t http://timestamp.comodoca.com/authenticode &quot;$(TargetPath)&quot;"
	/>
	<UserMacro
		Name="ConfigDir"
		Value="Release"
	/>
</VisualStudioPropertySheet>
