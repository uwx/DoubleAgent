;.OPTION EXPLICIT

.Define TargetDir=%TargetDir%
.Define InstallDir=%InstallDir%
.Define Platform=%Platform%
.Set Cabinet=ON
.Set Compress=ON
.Set GenerateInf=ON

.Set DiskDirectory1=%InstallDir%\%Platform%\
.Set CabinetName1=DaControl.cab
.Set MaxDiskSize=10485760
.Set MaxCabinetSize=10485760
.Set ReservePerCabinetSize=6144	; Reserved for signature

%TargetDir%\DaControl.ocx /INF=NO
%InstallDir%\DaControlOCX(%Platform%).inf DaControl.inf
