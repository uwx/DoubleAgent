<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Testing"
	IntermediateDirectory="$(LibDir)$(ConfigDir)$(PlatformDir)\Testing\$(ProjectName)\"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="..\..\Include;..\..\Util;..\..\Util\Atl;..\..\Util\Mfc;..\..\Core;..\..\Server;..\..\Resources;$(NOINHERIT)"
	/>
	<Tool
		Name="VCLinkerTool"
		ProgramDatabaseFile="$(TargetDir)$(TargetName).pdb"
		StripPrivateSymbols=""
		GenerateMapFile="false"
		DelayLoadDLLs="$(StopEvaluating)"
	/>
</VisualStudioPropertySheet>
