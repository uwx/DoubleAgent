<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="Testing"
	IntermediateDirectory="$(LibDir)$(ConfigDir)$(PlatformDir)\Testing\$(ProjectName)\"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(SolutionDir)\Include&quot;;&quot;$(SolutionDir)\Util&quot;;&quot;$(SolutionDir)\Util\Atl&quot;;&quot;$(SolutionDir)\Util\Mfc&quot;;&quot;$(SolutionDir)\Core&quot;;&quot;$(SolutionDir)\Server&quot;;&quot;$(SolutionDir)\Resources&quot;;$(NOINHERIT)"
	/>
	<Tool
		Name="VCLinkerTool"
		DelayLoadDLLs="$(StopEvaluating)"
		ProgramDatabaseFile="$(TargetDir)$(TargetName).pdb"
		StripPrivateSymbols=""
		GenerateMapFile="false"
	/>
</VisualStudioPropertySheet>
