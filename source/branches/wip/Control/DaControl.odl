	[helpstring("The status of a request sent to the Double Agent server")]
	typedef enum RequestStatus
	{
		[helpstring("The request was successfully completed")]
			RequestStatus_Success = 0,
		[helpstring("The request failed and an error code is available in the request object's Number property")]
			RequestStatus_Failed = 1,
		[helpstring("The request is on the queue")]
			RequestStatus_Pending = 2,
		[helpstring("The request was interrupted and an error code is available in the request object's Number property")]
			RequestStatus_Interrupted = 3,
		[helpstring("The request is currently in progress")]
			RequestStatus_InProgress = 4
	}	RequestStatus;

	const long RequestSuccess = RequestStatus_Success;
	const long RequestFailed = RequestStatus_Failed;
	const long RequestPending = RequestStatus_Pending;
	const long RequestInterrupted = RequestStatus_Interrupted;
	const long RequestInProgress = RequestStatus_InProgress;

